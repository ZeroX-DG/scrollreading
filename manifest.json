{

    "manifest_version": 2,
    "name": "ScrollReading",
    "version": "1.0",

    "description": "A reading aid you're looking for",

    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["jquery.js", "scrollreading.js"],
            "css": ["scrollreading.css"]
        }
    ]

} 
